package network.protocole.tickmap.reponse;

import java.security.cert.X509Certificate;

import generic.server.AReponse;

public class ReponseCertificat extends AReponse {
    
    private X509Certificate certificate;
    
    protected ReponseCertificat(String message,boolean successful) {
	super(message, successful);
	// TODO Auto-generated constructor stub
    }

    /**
     * 
     */
    private static final long serialVersionUID = 1L;
    public static ReponseCertificat OK(){
        return new ReponseCertificat("", true);
    }

    public static ReponseCertificat KO(String message){
        return new ReponseCertificat(message, false);
    }
}
